# Uncomment the next line to define a global platform for your project
  platform :ios, '9.0'

target 'pokedex' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for pokedex
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'SteviaLayout'
  pod 'Alamofire'
  pod 'Kingfisher'

  target 'pokedexTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking'
    pod 'RxTest'
  end

  target 'pokedexUITests' do
    # Pods for testing
    pod 'RxBlocking'
    pod 'RxTest'
  end

end
